rules_version = '2';
service azure.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      // Allow read/write only to authenticated users' own folders
      match /users/{userId}/{allUserPaths=**} {
        allow read, write: if request.auth != null && request.auth.uid == userId;
      }
    }
  }
}
